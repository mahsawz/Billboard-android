<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.billboard">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logooo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/billboard"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".ShowAppActivity" />
        <activity android:name=".ShowGiftActivity" />
        <activity android:name=".ShowGiftHistoryActivity" />
        <activity android:name=".BuyGiftActivity" />
    </application>

</manifest>
